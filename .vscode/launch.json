{
  "version": "0.2.0",
  "configurations": [
      {
          "type": "node",
          "request": "launch",
          "name": "Launch",
          "program": "${workspaceRoot}/index.js"
      },
      {
        "name": "Launch via npm",
        "type": "node",
        "request": "launch",
        "cwd": "${workspaceFolder}",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run-script", "start"]
      },      
      {
          "type": "node",
          "request": "attach",
          "name": "Attach",
          "port": 9229
      }
  ]
}